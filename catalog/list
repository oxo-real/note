sh   < ${$(get-newest-file $XDG_LOGS_HOME/upgrader/$HOST/package_lists/)%?}Q  ## make last character of newest file a Q and print that file (latest upgrade)
sh   $(command)  ## command substitution; gets the result of command
sh   $command <<- EOF >> $file\n multi\nline\ntext\nmlt\nEOF\n  ## input read until EOF (<< heredoc) to command, strip leading tabs from mlt (-), EOF can be any unique limit string
sh   $(( expression ))  ## arithmetic expansion; gets the result of $expression
sh   < $file  ## less $file (via READNULLCMD)
sh   [ "$var" ]  ## alias for: 'test "$var"'
sh   [[ "$var" =~ extended-regexp ]]  ## test with extended regular expression operator
sh   [[ $var ]]  ## extended version of [ operator; with additional string, assignment and arithmetic operations and improved functionality, readability and performance
sh   alacritty msg config font.size=18  ## current alacritty instance font size 18 (for screencasting)
sh   ~-  ## alias $OLDPWD
sh   ~+  ## alias $PWD
sh   alnum=$(printf '%s' "$ASCII" | cut --characters 1-16,27-33,60-65,92-95 --complement)  ## 62 alphanumeric characters from $ASCII
sh   alnum=$(printf '%s' "$ASCII" | grep --only-matching  . | sort | tr -d "\n" | cut --characters 1-33 --complement)  ## 62 alphanumeric characters from $ASCII
sh   amixer info  ## info about alsamixer device
sh   amixer scontrols  ## list of alsamixer controls
sh   amixer set Capture toggle  ## alsamixer toggle mic
sh   amixer set Capture toggle && amixer get Capture | grep '\[off\]' && notify-send "MIC switched OFF" || notify-send "MIC switched ON"  ## mute mic with visual status update
sh   amixer set Capture volume 0 nocap  ## alsamixer mute mic
sh   amixer set Internal Mic Capture volume 0 nocap  ## alsamixer mute mic
sh   amixer set Master playback volume 30 unmute  ## alsamixer unmute playback volume 30
sh   apropos . | fzf | awk '{print $1}' | xargs -r man  ## fzf search apropos and open manpage (man -k)
sh   apropos keyword  ## search man page names and descriptions
sh   arecord --vumeter stereo audio.wav  ## record wave audio
sh   ascii=$(for (( i=32; i<127; i++ )) do; printf "\\$(printf %03o "$i")"; done)  ## 95 printable ascii characters (0x20-0x7e 0d32-0d126)
sh   awk '!($1="")' $file  ## remove column 1
sh   awk '{a[i++]=$0} END {for (j=i-1; j>=0;) print a[j--] }'  ## reverse lines (emulate tac)
sh   awk '{for(i=1; i<=NF; i++) if($i=="version") print $(i), $(i+1)}' <<< $(echo 'a b c version 2 d e f')  ## extract string and following
sh   awk 'NF{NF-=1};1'  ## remove last column
sh   awk 'NR == 1; NR > 1 {print $0 | "sort -n"}'  ## sort stdin, omit first header line
